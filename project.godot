; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="long shot"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Clock="*res://autoloads/Clock.gd"
AudioManager="*res://autoloads/AudioManager/AudioManager.tscn"
RoomManager="*res://autoloads/RoomManager/RoomManager.tscn"

[display]

window/size/viewport_width=480
window/size/viewport_height=320
window/size/window_width_override=1440
window/size/window_height_override=960
window/stretch/mode="viewport"

[layer_names]

2d_physics/layer_1="World"
2d_physics/layer_2="Player"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.064991, 0.0778517, 0.108732, 1)
